{
    "name": "themsaid/nova-cashier-manager",
    "description": "Managing Stripe subscriptions inside the incredible Laravel Nova admin panel.",
    "keywords": [
        "laravel",
        "nova",
        "cashier",
        "stripe"
    ],
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "mohamed@laravel.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "stripe/stripe-php": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\CashierTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\CashierTool\\CashierToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
